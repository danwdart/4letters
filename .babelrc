{
    "presets": [
        [
            "@babel/preset-env",
            {
                "exclude": [
                    "transform-regenerator"
                ],
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        "dynamic-import-node",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-syntax-import-meta"
    ]
}
